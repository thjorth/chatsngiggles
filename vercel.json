{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/test.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "api/index.html"
        },
        {
            "src": "/test",
            "dest": "api/test.js"
        }
    ]
}